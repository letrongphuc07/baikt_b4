package B1;
public class GiaoDichDat extends GiaoDich {
    private String loaiDat;

    public GiaoDichDat(String maGiaoDich, String NgayNhapXuat, int donGia, double dienTich) {
        super(maGiaoDich, NgayNhapXuat, donGia, dienTich);
    }

    public GiaoDichDat(String loaiDat, String maGiaoDich, String NgayNhapXuat, int donGia, double dienTich) {
        super(maGiaoDich, NgayNhapXuat, donGia, dienTich);
        this.loaiDat = loaiDat;
    }

    public String getLoaiDat() {
        return loaiDat;
    }

    public void setLoaiDat(String loaiDat) {
        this.loaiDat = loaiDat;
    }

    @Override
    public String toString() {
        return "GiaoDichDat{"+ super.toString() + "loaiDat=" + loaiDat + '}';
    }
    
}

